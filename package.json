{
  "name": "fastify_prizma",
  "version": "1.0.0",
  "description": "Learning prisma + fastify together",
  "main": "index.js",
  "repository": "localhost",
  "author": "techies",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "set NODE_ENV=production TS_NODE_BASEURL=./dist && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"nodemon\"",
    "easy-dev": "tsnd --respawn --transpile-only --exit-child src/index.ts",
    "test": "tap --node-arg=--require=ts-node/register"
  },
  "devDependencies": {
    "@types/node": "^18.15.1",
    "concurrency": "^0.1.4",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "fastify": "^4.14.1",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^4.5.0",
    "fastify-swagger": "^5.2.0",
    "fastify-zod": "^1.2.0",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.20.4"
  }
}
